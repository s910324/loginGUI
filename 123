<script type="text/javascript">

    


    function result() {
        window.location = '/Account/Index?bulletin_cate=B';

    }
    function submitFm() {
        $('#page').val('1');
        document.forms["SearchFm"].submit();
    }

    function checkNoCalamity(value, value2)
    {
        if(value2 == "Y")
        {
            $('#'+value).hide();
            $('#'+value).find("input[type=radio]").attr("checked", false);
        }else {
            $('#'+value).show();
        }
    }

    function CalculateClickRate(Sno)
    {
        $.ajax({
            type: "POST",
            url: "/BulletinBoard/CalculateClickRate",
            data: "Sno=" + Sno ,
            success: function (msg) {
                $('#spanValue_'+Sno).html(msg);
                $('#span_'+Sno).hide();
                $('#span2_'+Sno).show();
            }
        });
    }

</script>

  

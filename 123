    <script type="text/javascript">
        $(function () {
            var $block = $('#abgne_fade_pic'),
                $ad = $block.find('.ad'),
                showIndex = 0,			// 預設要先顯示那一張
                fadeOutSpeed = 1800,	// 淡出的速度
                fadeInSpeed = 1800,		// 淡入的速度
                defaultZ = 10,			// 預設的 z-index
                isHover = false,
                timer, speed = 3500;	// 計時器及輪播切換的速度
            // 先把其它圖片的變成透明
            $ad.css({
                opacity: 0,
                zIndex: defaultZ - 1
            }).eq(showIndex).css({
                opacity: 1,
                zIndex: defaultZ
            });

            // 組出右下的按鈕
            var str = '';
            for (var i = 0; i < $ad.length; i++) {
                str += '<a href="#">' + (i + 1) + '</a>';
            }

            var $controlA = $('#abgne_fade_pic').append($('<div class="control">' + str + '</div>').css('zIndex', defaultZ + 1)).find('.control a');
            // 當按鈕被點選時
            // 若要變成滑鼠滑入來切換時, 可以把 click 換成 mouseover
            $controlA.click(function () {
                // 取得目前點擊的號碼
                showIndex = $(this).text() * 1 - 1;
                // 顯示相對應的區域並把其它區域變成透明
                $ad.eq(showIndex).stop().fadeTo(fadeInSpeed, 1, function () {
                    if (!isHover) {
                        // 啟動計時器
                        timer = setTimeout(autoClick, speed);
                    }
                }).css('zIndex', defaultZ).siblings('a').stop().fadeTo(fadeOutSpeed, 0).css('zIndex', defaultZ - 1);
                // 讓 a 加上 .on
                $(this).addClass('on').siblings().removeClass('on');
                return false;
            }).focus(function () {
                $(this).blur();
            }).eq(showIndex).addClass('on');
            $ad.hover(function () {
                isHover = true;
                // 停止計時器
                clearTimeout(timer);
            }, function () {
                isHover = false;
                // 啟動計時器
                timer = setTimeout(autoClick, speed);
            })
            // 自動點擊下一個
            function autoClick() {
                if (isHover) return;
                showIndex = (showIndex + 1) % $controlA.length;
                $controlA.eq(showIndex).click();
            }
            // 啟動計時器
            timer = setTimeout(autoClick, speed);
        });
        function MM_swapImgRestore() { //v3.0
            var i, x, a = document.MM_sr; for (i = 0; a && i < a.length && (x = a[i]) && x.oSrc; i++) x.src = x.oSrc;
        }
        function MM_preloadImages() { //v3.0
            var d = document; if (d.images) {
                if (!d.MM_p) d.MM_p = new Array();
                var i, j = d.MM_p.length, a = MM_preloadImages.arguments; for (i = 0; i < a.length; i++)
                    if (a[i].indexOf("#") != 0) { d.MM_p[j] = new Image; d.MM_p[j++].src = a[i]; }
            }
        }

        function MM_findObj(n, d) { //v4.01
            var p, i, x; if (!d) d = document; if ((p = n.indexOf("?")) > 0 && parent.frames.length) {
                d = parent.frames[n.substring(p + 1)].document; n = n.substring(0, p);
            }
            if (!(x = d[n]) && d.all) x = d.all[n]; for (i = 0; !x && i < d.forms.length; i++) x = d.forms[i][n];
            for (i = 0; !x && d.layers && i < d.layers.length; i++) x = MM_findObj(n, d.layers[i].document);
            if (!x && d.getElementById) x = d.getElementById(n); return x;
        }

        function MM_swapImage() { //v3.0
            var i, j = 0, x, a = MM_swapImage.arguments; document.MM_sr = new Array; for (i = 0; i < (a.length - 2) ; i += 3)
                if ((x = MM_findObj(a[i])) != null) { document.MM_sr[j++] = x; if (!x.oSrc) x.oSrc = x.src; x.src = a[i + 2]; }
        }

        function LoginSubmitFm() {
            var strUserID = $('#strUserID').val();
            var strPwd = $('#strPwd').val();
            if (strUserID == "")
            {
                alert('請輸入帳號');
                return false;
            }

            if (strPwd == "") {
                alert('請輸入密碼');
                return false;
            }

            $('#Fm').submit();
        }

        function LogOutFm(url) {
            if (confirm("確定登出嗎？")) {
                window.location.href = url;
            }
        }

        jQuery(window).load(function () {
            //執行倒數計時
            if ($('#LoginFlag').val() != "") {
                $('#counter').show();
                clock();
            }
        });

        function logOut() {
            alert('您已閒置過久，故系統已將您登出');
            window.location.href = '/Account/LogOut';
        }

        function enterKeyDown(event) {
            if (event.keyCode == 13) {
                LoginSubmitFm();
            }
        }

        function clearWord(value) {
            if (value == '請輸入關鍵字') {
                document.getElementById('search').value = "";
            }
        }
        function FogPwd(url) {
            url += "?width=900&height=450";
            tb_show('忘記密碼', url, null);
        }

        function SearchData() {

            var value = $('#search').val();

            if (value != "請輸入關鍵字" && value != "") {
                var url = '/Map/SearchList?search='+ value;
                window.location.href = url;
            } else {
                alert("請輸入您要搜尋的關鍵字");
                return false;
            }
        }


        function submitCalamity(value, url)
        {

            if(CheckValue("rdl_"+value) != "")
            {
                if(CheckValue("rdl_"+value) == "Y"){
                    alert("您點選了有災損，系統將自動幫您導至校安即時通首報。");
                    window.location.href=url;
                }else{
                    ////點選無災損
                    if(CheckValue('system'+value) == "")
                    {
                        alert(' •請選擇您的學制');
                    }

                    var strUserId = "";
                    var strAccount = "";
                    $.ajax({
                        type: "POST",
                        url: "/Calamity/SetCalamityStatus",
                        contentType:"application/x-www-form-urlencoded",
                        data: "CalamityId=" + value + "&CalamityId=" + value + "&UserId=" + strUserId + "&Account=" + strAccount + "&Status=" + CheckValue("rdl_"+value) + "&system="+ CheckValue("system"+value),
                        success: function (msg) {
                            if(msg == "1")
                            {
                                alert("已回報無災損");
                                history.go(0);
                                //alert("已回報無災損，因應模擬訓練，系統將自動幫您登出");
                                //window.location.href = "/Account/LogOut";
                            }
                            else
                            {
                                alert("回報無災損失敗");
                                history.go(0);
                            }
                            //alert(msg);
                            
                            //history.go(0);

                        }
                    });

                }
            }
            else
            {
                alert(" •請點選【是否有無災損】");
            }
        }

        //隱私權
        function ShowPrivacy(url) {
            url += "?width=900&height=450";
            tb_show('隱私權政策', url, null);
        }

    </script>

</head>
